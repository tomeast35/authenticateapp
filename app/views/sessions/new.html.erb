<h1>ログイン</h1>

<%= form_for(:session, url: sessions_path) do |f| %>
<p><%= f.label :email %></p>
<p><%= f.text_field :email %></p>
<p><%= f.label :password %></p>
<p><%= f.text_field :password %></p>

<%= f.submit "ログインする" %>
<% end %>

<p>登録はコチラ<%= link_to "新規登録", signup_path %></p>
